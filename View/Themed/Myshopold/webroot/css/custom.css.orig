body{
    background: url("../img/background.jpg") repeat;
    /*background: url("../image/layout.jpg");*/
    padding-top:50px;
}

a{
    color: #ffffff;
}

a:hover{
    text-decoration: none;
    color: #0066ff;
}

.check-one{
    display: inline-block;
    font-size: 50px;
    color: #428a62;
}

.check-one > a:hover{
    color: #0066ff;
}

.check-two{
    display: inline-block;
    color: #428a62;
    font-weight: bold;
    padding-right: 10px;
}

@media only screen and (max-width: 767px) {
    .coustome{
        position: absolute;
        top: -10px !important;
        left: 85px !important;
        width: 50%;
        border: none;
    }
    .coustomr-header {
        position: relative;
    }
}


.hd{
    color: #ffffff;
    font-size: 18px;
}

.txt{
    color: #189c2b;
}

.takeaway{
    background-color: #ffffff;
    border-radius: 8px;
    text-align: center;
	font-family:"Trebuchet MS", Arial, Helvetica, sans-serif
}

.cuisines{
    padding-left: 10px;
    text-align: left;
}

.shop{
    color: #ffffff;
}

.navbar-inverse{
    background-color: #428a62;
    border-color: #428a62;
    /*opacity: 0.9;*/
}

.navbar-inverse
.navbar-nav > li > a{
    color: #ffffff;
}

.navbar-inverse
.navbar-brand{
    color: #ffffff;
}

.carousel-text
{
    position: absolute;
    font-size: 100%;
    font-weight: bold;
    margin-top: 13%;
    margin-left: 24%;
    color: #ffffff;
    z-index: 1000;
}

.carousel-search
{
    position: absolute;         /* relative */
    width: 38%;
    font-size: 100%;        /* 1.5em */
    /*font-weight: bold;*/
    padding: 5px 5px 5px 5px;
    /*margin: 30px 10px 10px -70px;*/
    /*margin-left: -12px;         *//* -10px; */
    margin-top: 20%;        /* 20px */
    margin-left: 20%;
    /*color: #ffffff;*/
    background-color: #3a7d50;      /* da2127 */
    text-shadow: 0px 1px 2px #bbb;
    -webkit-box-shadow: 0px 2px 4px #888;
    -moz-box-shadow: 0px 2px 4px #888;
    box-shadow: 0px 2px 4px #888;
    z-index: 1000;
    margin-bottom: -300px;
    /*font-family: fontfive;*/
}

.contact-box{
    background-color: #005526;
    border: 1px solid #008f41;
    margin-bottom: 2px;
    color: #ffffff
}

.footer-one{
    background-color: #096733;
    padding-top: 20px;
    padding-bottom: 20px;
}

.footer-one .container{
    padding-left: 0px;
    padding-right: 0px;
}

.nav > li > a:hover,
.nav > li > a:focus{
    background: none;
    color: #ffffff;
}

.logon-box{
    background-color: #EEEEEE; opacity: .8; padding: 10px; border-radius: 8px;
}
